        <Menu mnemonicParsing="false" text="File">
          <items>
            <MenuItem fx:id="mainMenuExitButton" mnemonicParsing="false" text="Exit" />
          </items>
        </Menu>
            <Menu fx:id="mainMenuHelpButton" mnemonicParsing="false" text="Help" />
      </menus>
    </MenuBar>
  </top>
   <center>
      <AnchorPane fx:id="centerAnchorPane" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" BorderPane.alignment="CENTER" stylesheets="@MainStyle.css"/>
   </center>
